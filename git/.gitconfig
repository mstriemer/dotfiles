[user]
    name = Mark Striemer
    email = mstriemer@gmail.com
[alias]
    ci = commit
    st = status
    br = branch
    co = checkout
[color]
    ui = auto
[color "branch"]
    current = yellow reverse
    local = yellow
    remote = green
[color "diff"]
    meta = yellow bold
    frag = magenta bold
    old = red bold
    new = green bold
[color "status"]
    added = yellow
    changed = green
    untracked = cyan
[core]
    excludesfile = /Users/Mark/.gitignore
	editor = /usr/bin/vim
